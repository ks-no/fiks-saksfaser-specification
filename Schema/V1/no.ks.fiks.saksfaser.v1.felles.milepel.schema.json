            ]
        },
        "frist": {
            "type": "date"
        },
        "utfoert": {
            "type": "date"
        },
        "planlagtTil": {
            "type": "date"
        },
        "intern": {
            "type": "boolean"
        },
        "referanseJournalpost": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.referansejournalpost.schema.json"
        },
        "referanseDokument": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.referansedokumentfil.schema.json"
        },
        "erStart": {
            "type": "boolean"
        },
        "erSlutt": {
            "type": "boolean"
        }
