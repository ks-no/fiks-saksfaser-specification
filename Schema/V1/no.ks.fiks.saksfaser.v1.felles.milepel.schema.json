{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "no.ks.fiks.saksfaser.v1.felles.milepel.schema.json",
    "title": "Milepel",
    "type": "object",
    "properties": {
        "milepelId": {
            "type": "string"
        },
        "beskrivelse": {
            "type": "string"
        },
        "kategori": {
            "type": "object",
