{
    "$schema": "https://json-schema.org/draft/2020-12/schema",
    "$id": "no.ks.fiks.saksnummer.v1.felles.byggesak.schema.json",
    "title": "Byggesak",
    "type": "object",
    "properties": {
        "saksnummer": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.saksnummer.schema.json"
        },
        "systemid": {
            "type": "string"
        },
        "tittel": {
            "type": "string"
        },
        "tiltakstype": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.tiltakstype.schema.json"
        },
        "adresse": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.adresse.schema.json"
        },
        "registrertDato": {
            "type": "date"
        },
        "avsluttetDato": {
            "type": "date"
        },
        "gjeldendePlaner": {
            "type": "array",
            "items": {
                "$ref": "no.ks.fiks.saksfaser.v1.felles.nasjonalarealplanid.schema.json"
            }
        },
        "matrikkelopplysninger": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.matrikkelopplysninger.schema.json"
        },
        "posisjon": {
            "$ref": "no.ks.fiks.saksfaser.v1.felles.matrikkelopplysninger.schema.json"
        },
        "referanseAndreSaker": {
            "type": "array",
            "items": {
                "$ref": "no.ks.fiks.saksfaser.v1.felles.saksnummer.schema.json"
            }
        },
        "referanseKlagesaker": {
            "type": "array",
            "items": {
                "$ref": "no.ks.fiks.saksfaser.v1.felles.saksnummer.schema.json"
            }
        }
    },
    "required": [
        "saksnummer",
        "registrertDato"
    ]
}